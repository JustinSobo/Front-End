{"components/common/footer":"{{#if banners.bottom}}\n<div class=\"banners\" data-banner-location=\"bottom\">\n    <div class=\"banner\">\n        {{{limit banners.bottom 1}}}\n    </div>\n</div>\n{{/if}}\n<footer class=\"footer\">\n    <div class=\"themevale_mainFooter\">\n        <div class=\"themevale_container\">\n            <div class=\"themevale_topFooter\">\n                <div class=\"themevale_service\">\n                    <div class=\"service-item\">\n                        <div class=\"service-wrapper\"><p class=\"text\">Free Express Shipping</p> </div>\n                    </div>\n                    <div class=\"service-item\">\n                        <div class=\"service-wrapper\"><p class=\"text\">Free In-Store Returns</p> </div>\n                    </div>\n                    <div class=\"service-item\">\n                        <div class=\"service-wrapper\"><p class=\"text\">Student Discount 40% Off</p> </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"themevale_middleFooter\">\n                <div class=\"themevale-logoFooter\">\n                    <a href=\"index.html\" class=\"image-logo\">\n                        logo footer\n                        <img class=\"logo-footer-image lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/logo-1.png\" alt=\"logo-footer\">\n                    </a>\n                </div>\n                <div class=\"themevale-infoFooter\">\n                    <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\">\n                        <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>\n                        <div class=\"footer-info-list\">\n                            <div class=\"store-address\">{{nl2br settings.address}}</div>\n                            {{#if settings.phone_number}}\n                            <div class=\"store-call\">\n                                <span>{{lang 'footer.call_us' phone_number=settings.phone_number}}</span>\n                            </div>\n                            {{/if}}\n                            {{> components/common/social-links}}\n                        </div>\n                    </div>\n                    <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\">\n                        <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\n                        <ul class=\"footer-info-list\">\n                            {{#each categories}}\n                                <li>\n                                    <a href=\"{{url}}\">{{name}}</a>\n                                </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                    <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\">\n                        <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h5>\n                        <ul class=\"footer-info-list\">\n                            {{#each shop_by_brand}}\n                                <li>\n                                    <a href=\"{{url}}\">{{name}}</a>\n                                </li>\n                            {{/each}}\n                            <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                        </ul>\n                    </div>\n                    <div class=\"footer-info-col footer-info-col--small footer-dropdownmobile\">\n                        <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\n                        <ul class=\"footer-info-list\">\n                            <li><a href=\"/contact-us/\">Contact Us</a></li>\n                            <li><a href=\"/about-us/\">About Us</a></li>\n                            <li><a href=\"/shipping-returns/\">Shipping & Returns</a></li>\n                            <li><a href=\"/faqs/\">FAQs</a></li>\n                            <li><a href=\"/press-release/\">Press Release</a></li>\n                            \n                            <li><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a></li>\n                        </ul>\n                    </div>\n                    <div class=\"footer-info-col\">\n                        {{#if settings.show_newsletter_box}}\n                            {{> components/common/subscription-form}}\n                        {{/if}}\n                        {{> components/common/payment-icons}}\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"themevale_bottomFooter\">\n        <div class=\"themevale_container\">\n            {{#if theme_settings.show_powered_by}}\n                <div class=\"footer-copyright\">\n                    <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} Powered by <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" target=\"_blank\" rel=\"noopener\">BigCommerce</a></p>\n                </div>\n            {{/if}}\n            <div class=\"themevale_logo\">\n                <a href=\"https://www.themevale.com/\" target=\"_blank\" rel=\"noopener\">\n                    <span class=\"icon\"><img class=\"lazyload\" data-src=\"{{cdn 'img/themevale-retina-logo.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"\"></span>\n                    <span class=\"text\">Bigcommerce Themes by Themevale</span>\n                </a>\n            </div>\n        </div>\n    </div>\n</footer>\n{{#if theme_settings.themevale_BackTop}}\n    <div id=\"back-to-top\">\n        <a href=\"javascript:void(0)\">to top<svg class=\"icon\"><use xlink:href=\"#icon-arrow-down\"></use></svg></a>\n    </div>\n{{/if}}\n","components/common/payment-icons":"{{#if theme_settings.show_payment-image}}\n    <div class=\"payment-wrapper\">\n        <img class=\"lazyload\" data-src=\"{{cdn 'img/payment.png'}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"payment\">\n    </div>\n{{/if}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        {{display_name}}\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        {{display_name}}\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n"}
