{"components/cms/brand":"\r\n<div id=\"brand\" class=\"owl-carousel'>\r\n\t\t{{#if (lang 'webibrand.logo1.image') '!=' ' '}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'http://bullseyegun.com/product_images/theme_images/brand-1.jpg')}}\"  alt=\"{{title}}\"  /></a>\r\n\t\t{{else}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'webibrand.logo1.placeholderImage')}}\" alt=\"{{title}}\"  /></a>\r\n\t\t{{/if}}\r\n\t\t\r\n\t\t{{#if (lang 'webibrand.logo1.image') '!=' ' '}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'http://bullseyegun.com/product_images/theme_images/brand-2.jpg')}}\"  alt=\"{{title}}\"  /></a>\r\n\t\t{{else}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'webibrand.logo2.placeholderImage')}}\" alt=\"{{title}}\"  /></a>\r\n\t\t{{/if}}\r\n\t\t\r\n\t\t{{#if (lang 'webibrand.logo1.image') '!=' ' '}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'http://bullseyegun.com/product_images/theme_images/brand-3.jpg')}}\"  alt=\"{{title}}\"  /></a>\r\n\t\t{{else}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'webibrand.logo3.placeholderImage')}}\" alt=\"{{title}}\"  /></a>\r\n\t\t{{/if}}\r\n\t\t\r\n\t\t{{#if (lang 'webibrand.logo1.image') '!=' ' '}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'http://bullseyegun.com/product_images/theme_images/brand-5.jpg')}}\"  alt=\"{{title}}\"  /></a>\r\n\t\t{{else}}\r\n\t\t<a href=\"{{url}}\"><img class=\"img-fluid lazyload\" data-sizes=\"auto\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn (lang 'webibrand.logo5.placeholderImage')}}\" alt=\"{{title}}\"  /></a>\r\n\t\t{{/if}}\r\n\t\t\r\n\t\t\r\n\t</div>\r\n</div>\r\n\r\n\r\n\r\n<script type=\"text/javascript\">\r\n    $(document).ready(function() {\r\n    $(\"#brand\").owlCarousel({\r\n    itemsCustom : [\r\n    [0, 2],\r\n    [600, 4],\r\n    [768, 5],\r\n    [1200, 6],\r\n    [1850, 7]\r\n    ],\r\n      autoPlay: 2500,\r\n      loop: true,\r\n      navigationText: ['<i class=\"fa fa-angle-left\" aria-hidden=\"true\"></i>', '<i class=\"fa fa-angle-right\" aria-hidden=\"true\"></i>'],\r\n      navigation : false,\r\n      pagination:false\r\n    });\r\n    });\r\n</script>","components/cms/foot-about":"<div class=\"foot-about\">\r\n{{#if page_type '===' 'default'}}\r\n    <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n        {{> components/common/store-logo}}\r\n    </div>\r\n{{else}}\r\n    <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n        {{> components/common/store-logo}}\r\n    </div>\r\n{{/if}}\r\n<p>Since 1976, Bullseye Guns have served American communities by providing quality tactical, sporting and personal-protection firearms for our citizens. We also provide the expertise and training to use guns responsibly and enjoy the fun—and peace of mind—of gun ownership.\r\n\r\n<br><br>Rely on Bullseye Guns for gun licensing, repairs, antique gun restoration and upgrades guided by master gunsmiths. Plus, take advantage of training and skills-building at our indoor shooting range, competitive target-shooting leagues, talks by experts, events and more.</p>\r\n</div>","components/common/footer":"{{#if banners.bottom}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.bottom 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n     <!-- Left banner -->\r\n               <div class=\"container\"> {{> components/cms/brand}}</div>\r\n        <hr class=\"foothr\">\r\n    <div class=\"container\">\r\n        <div class=\"row\">\r\n            <article class=\"col-md-4 col-12 fooab\" data-section-type=\"footer-webPages\">\r\n                    <div id=\"social\">\r\n                    \r\n                     {{> components/cms/foot-about }}\r\n                     {{> components/common/social-links}}\r\n                     \r\n                 </div>\r\n            </article>\r\n            \r\n        \r\n        <section class=\"footer-infon col-md-8 col-12\">\r\n            <div class=\"row\">\r\n            <article class=\"col-md-4 col-12 fborder\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}\r\n                    <button type=\"button\" class=\"toggle plus-button collapsed\" data-toggle=\"collapse\" data-target=\"#nevigate\"></button>\r\n                </h5>\r\n                <ul class=\"footer-info-list collapse navbar-collapse\" id=\"nevigate\">\r\n                    {{#each pages}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li>\r\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                    </li>\r\n                </ul>\r\n            </article>\r\n\r\n           <!--  <article class=\"col-lg-3 col-xs-12\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}\r\n                    <button type=\"button\" class=\"toggle plus-button collapsed\" data-toggle=\"collapse\" data-target=\"#fcate\"></button>\r\n                </h5>\r\n                <ul class=\"footer-info-list collapse navbar-collapse\" id=\"fcate\">\r\n                    {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                </ul>\r\n            </article> -->\r\n\r\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\r\n            <article class=\"col-md-4 col-12 fborder\" data-section-type=\"footer-brands\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}\r\n                    <button type=\"button\" class=\"toggle plus-button collapsed\" data-toggle=\"collapse\" data-target=\"#pbrands\"></button>\r\n                </h5>\r\n                <ul class=\"footer-info-list collapse navbar-collapse\" id=\"pbrands\">\r\n                    {{#each shop_by_brand}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n            {{/and}}\r\n\r\n            {{#if settings.address}}\r\n            <article class=\"col-md-4 col-12 fborder\" data-section-type=\"storeInfo\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}\r\n                    <button type=\"button\" class=\"toggle plus-button collapsed\" data-toggle=\"collapse\" data-target=\"#cinfo\"></button>\r\n                </h5>\r\n                <div class=\"collapse navbar-collapse\" id=\"cinfo\">\r\n                    <img class=\"img-responsive pull-left\" src=\"https://cdn8.bigcommerce.com/s-33xcm4zhia/product_images/uploaded_images/location.png?t=1530191969&_ga=2.183814191.8881157.1530009852-1542549271.1530009852\"><address>{{nl2br settings.address}}</address>\r\n                    {{#if settings.phone_number}}\r\n                        <img class=\"img-responsive pull-left\" src=\"https://cdn8.bigcommerce.com/s-33xcm4zhia/product_images/uploaded_images/phone.png?t=1530191970&_ga=2.183814191.8881157.1530009852-1542549271.1530009852\"><span class=\"foot-phone\">{{lang 'footer.call_us' phone_number=settings.phone_number}}</span>\r\n                    {{/if}}\r\n                    {{> components/common/payment-icons}}\r\n                </div>\r\n            </article>\r\n            {{/if}}\r\n        </div>\r\n        </section>\r\n        {{#if theme_settings.geotrust_ssl_common_name}}\r\n            <div class=\"footer-geotrust-ssl-seal\">\r\n                {{> components/common/geotrust-ssl-seal}}\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n    </div>\r\n<div class=\"footcopy\">\r\n    <div class=\"container\">\r\n        <div class=\"foot-bottom text-center\"> \r\n        {{#if theme_settings.show_powered_by}}\r\n                <span class=\"powered-by\">Powered by <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></span>\r\n        {{/if}}\r\n        {{#if theme_settings.show_copyright_footer}}\r\n                <span class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </span>\r\n        {{/if}}\r\n    </div>\r\n    </div>   \r\n</div>\r\n     <a href=\"\" id=\"scroll\" title=\"Scroll to Top\" style=\"display: block;\">\r\n       <img src=\"http://bullseyegun.com/product_images/uploaded_images/scroll.png\" alt=\"\">\r\n    </a>\r\n</footer>\r\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n    <tr>\r\n        <td width=\"135\" align=\"center\" valign=\"top\">\r\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\r\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\r\n        </td>\r\n    </tr>\r\n</table>\r\n","components/common/payment-icons":"{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa}}\r\n        <div class=\"footer-payment-icons\">\r\n            {{#if show_accept_amex}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_discover}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_mastercard}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_paypal}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_visa}}\r\n                <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n            {{/if}}\r\n        </div>\r\n    {{/or}}\r\n{{/with}}\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        <h5 class=\"text-capitalize\">{{lang 'footer.social'}}</h5>\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n"}
