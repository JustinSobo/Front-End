{"components/common/navigation":"\r\n\r\n    \r\n         <div class=\"col-sm-7 col-lg-7 col-12 hsearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n            {{> components/common/quick-search}}\r\n        </div>\r\n       <!--  <li class=\"navUser-item\">\r\n            <a class=\"navUser-action navUser-action--quickSearch\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">{{lang 'common.search'}}</a>\r\n        </li> -->\r\n        <div class=\"text-right col-sm-2 col-lg-2 top-right\">\r\n            <li class=\"navUser-item head-user user-info list-inline-item huserb\">\r\n                <a class=\"navUser-action userinfoa\" data-cart-preview=\"\" data-dropdown=\"account-dropdown\" data-options=\"align:right\">\r\n                    <ul class=\"navUser-item-cartLabel list-inline text-left\">\r\n                        <svg width=\"24px\" height=\"24px\"><use xlink:href=\"#user\"></use></svg>\r\n                    </ul>\r\n                </a>\r\n                <div class=\"dropdown-menu user-down\" id=\"account-dropdown\" data-dropdown-content aria-hidden=\"false\" >\r\n                        {{#if customer}}\r\n                            <a class=\"navUser-action dropdown-menu-item\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\r\n                            <a class=\"navUser-action dropdown-menu-item\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                        {{else}}\r\n                            <a class=\"navUser-action dropdown-menu-item\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\r\n                            {{#if settings.account_creation_enabled}}\r\n                                <a class=\"navUser-action dropdown-menu-item\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                            {{/if}}\r\n                        {{/if}}\r\n                </div>\r\n            </li><li class=\"navUser-item navUser-item--cart\">\r\n                <a\r\n                    class=\"navUser-action\"\r\n                    data-cart-preview\r\n                    data-dropdown=\"cart-preview-dropdown\"\r\n                    data-options=\"align:right\"\r\n                    href=\"{{urls.cart}}\">\r\n                    <span class=\"navUser-item-cartLabel\"><svg width=\"28px\" height=\"26px\"><use xlink:href=\"#hcart\"></use></svg></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                </a>\r\n\r\n                <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n            </li>\r\n        </div>\r\n","components/common/quick-search":"<div>\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field wb-search d-flex\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <input class=\"button--primary wb-search-btn\" align=\"center\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\r\n                \">\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n"}
